{
  "git.terminalGitEditor": true,
  "git.verboseCommit": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "nuxt.isNuxtApp": false,
  "tailwindCSS.includeLanguages": {
    "typescript": "html",
    "typescriptreact": "html"
  },
  "tailwindCSS.emmetCompletions": false,
  "tailwindCSS.hovers": false,
  "tailwindCSS.codeActions": false,
  "tailwindCSS.suggestInHeadlessMode": false,
  "html.customData": [
    "./node_modules/@esri/calcite-components/dist/docs/vscode.html-custom-data.json"
  ],
  "css.customData": [
    "./node_modules/@esri/calcite-components/dist/docs/vscode.css-custom-data.json"
  ]
}
